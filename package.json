{
  "name": "newt-client-js",
  "version": "1.2.1",
  "description": "JavaScript SDK for Newt's API",
  "main": "./dist/cjs/newtClient.js",
  "module": "./dist/esm/newtClient.js",
  "jsdelivr": "dist/umd/newtClient.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "newt"
  ],
  "author": "Newt, Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Newt-Inc/newt-client-js.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "yarn clean && rollup -c",
    "clean": "rimraf dist",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier --check ./src",
    "fix:eslint": "prettier --fix ./src",
    "fix:prettier": "prettier --write ./src",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tests/tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "axios": "^0.25.0",
    "axios-retry": "^3.2.4",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.7.8",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.3",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^27.1.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  }
}
