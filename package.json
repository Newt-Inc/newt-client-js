{
  "name": "newt-client-js",
  "version": "1.1.0",
  "description": "JavaScript SDK for Newt's API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Newt, Inc.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Newt-Inc/newt-client-js.git"
  },
  "scripts": {
    "build": "tsc",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier --check ./src",
    "fix:eslint": "prettier --fix ./src",
    "fix:prettier": "prettier --write ./src",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tests/tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "qs": "^6.10.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/node": "^16.7.8",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.3",
    "prettier": "^2.3.2",
    "ts-jest": "^27.1.0",
    "typescript": "^4.4.2"
  }
}
